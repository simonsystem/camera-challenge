{
  "presets": [
    "@babel/preset-react",
    ["@babel/preset-env", {
      "loose": true,
      "targets": {"node": "current"}
    }]
  ],
  "plugins": [
    ["@babel/plugin-proposal-decorators", { "legacy": true }],
    ["@babel/plugin-proposal-class-properties", { "loose" : true }],
    "@babel/plugin-proposal-do-expressions"
  ],
  "retainLines": true
}
