    "@babel/preset-env": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.10.1",
    "@types/express": "^4.17.6",
    "@types/multer": "^1.4.3",
    "@types/nodemailer": "^6.4.0",
    "@types/nodemon": "^1.19.0",
