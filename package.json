    "@types/source-map": "0.5.2",
    "@types/tmp": "^0.2.0",
    "@types/util.promisify": "^1.0.4",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "concurrently": "^5.2.0",
    "css-loader": "^3.6.0",
    "debug": "^4.1.1",
