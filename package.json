{
  "name": "camera-challenge",
  "version": "0.0.1",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf web dist",
    "test": "jest",
    "build": "concurrently 'npm:build:*'",
    "build:web": "webpack --env.production --env.web",
    "build:node": "webpack --env.production",
    "watch": "concurrently 'npm:watch:*'",
    "watch:web": "webpack-dev-server --env.web --port 8080 --host 127.0.0.1",
    "watch:node": "webpack --watch",
    "present": "concurrently 'npm:present:*'",
    "present:web": "webpack-dev-server --env.production --env.web --port 8080 --host 0.0.0.0 --https",
    "present:node": "webpack --env.production --watch",
    "start": "concurrently 'npm:start:*'",
    "start:node": "node ./dist/index.js",
    "start:web": "http-server ./web --port 8080 -a 0.0.0.0 --proxy http://127.0.0.1:3000/"
  },
  "devDependencies": {
